
1 1500 <= while
    swap
    dup 15 % 0 = if
        "FizzBuzz" . nl
        drop drop
    else
        drop drop
        dup 3 % 0 = if
            "Fizz" . nl
            drop drop
        else
            drop drop
            dup 5 % 0 = if
                "Buzz" . nl
                drop drop
            else
                drop drop
                dup
                . nl
            end
        end
    end
    1 +
    swap
end 